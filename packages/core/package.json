{
  "name": "@show-env/core",
  "private": false,
  "version": "0.0.2",
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "create-toc": "doctoc ./README.md --github",
    "transpile": "babel src -d dist --copy-files",
    "modules:install": "npm install",
    "modules:remove": "rm -rf node_modules",
    "modules:reset": "npm run modules:remove && npm run modules:install",
    "modules:update": "npm-check --update"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6"
  }
}
