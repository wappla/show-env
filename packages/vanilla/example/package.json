{
  "name": "@show-env/example",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "npm run build -- --watch",
    "modules:install": "npm install",
    "modules:remove": "rm -rf node_modules",
    "modules:reset": "npm run modules:remove && npm run modules:install",
    "modules:update": "npm-check --update"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "laravel-mix": "^5.0.4",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@show-env/vanilla": "^0.0.1"
  }
}
